  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.1",
    "chai": "^3.2.0",
    "coveralls": "^2.11.12",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "fs-extra": "^0.30.0",
    "got": "^6.7.1",
    "husky": "^0.11.9",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.0",
